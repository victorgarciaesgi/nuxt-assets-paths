{
  "name": "nuxt-assets-paths",
  "version": "0.1.8",
  "description": "Autocomplete for nuxt assets, icons and images paths",
  "main": "lib/module.js",
  "keywords": [
    "nuxt assets paths",
    "nuxt typed assets",
    "nuxt typed icons",
    "nuxt typed icons paths",
    "nuxt icon paths",
    "nuxt image paths"
  ],
  "scripts": {
    "dev": "tsc -p ./tsconfig.json --pretty --watch",
    "build": "rimraf lib && rimraf types && tsc -p ./tsconfig.json --pretty",
    "copy-files": "copyfiles -u 1 ./src/**/*.js ./lib",
    "prepublish": "yarn build"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib",
    "types/index.d.ts",
    "types/types.d.ts",
    "types/vue.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://victorgarciaesgi@github.com/victorgarciaesgi/nuxt-assets-paths.git"
  },
  "author": {
    "name": "Victor Garcia",
    "url": "https://github.com/victorgarciaesgi"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/victorgarciaesgi/nuxt-assets-paths/issues"
  },
  "typings": "types/index.d.ts",
  "dependencies": {
    "chalk": "^4.1.1",
    "lodash": "^4.17.21",
    "log-symbols": "^5.0.0",
    "prettier": "^2.3.1"
  },
  "devDependencies": {
    "@nuxt/types": "^2.15.7",
    "@types/lodash": "^4.14.165",
    "@types/node": "^15.12.4",
    "@types/prettier": "^2.3.0",
    "copyfiles": "^2.4.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.4"
  }
}
